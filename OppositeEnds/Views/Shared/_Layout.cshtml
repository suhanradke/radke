    <script type="text/javascript">

        $(document).ready(function () {
            initialize();
        });
   function initialize() {
     var mapOptions = {
         center: new google.maps.LatLng(31.1040879,-91.3000999),
       zoom: 15,
       mapTypeId: google.maps.MapTypeId.ROADMAP
     };
     var map = new google.maps.Map(document.getElementById("map_canvas"),
       mapOptions);
     // create a marker
     var latlng = new google.maps.LatLng(31.1040879,-91.3000999);
     var marker = new google.maps.Marker({
       position: latlng,
       map: map,
       title: 'OppositeEnds'
     });
   }
    </script>
    <script type="text/javascript">
            $(window).scroll(function () {
                if ($(document).scrollTop() > 50) {
                    $('nav').addClass('shrink');
                } else {
                    $('nav').removeClass('shrink');
                }
            });
    </script>

